#!/Users/jacquesmenu/musicformats-git-dev/build/bin/mfsl


# ----------------------------------------------------------
# the MusicFormats tool to be used
# ----------------------------------------------------------

tool : xml2ly ;


# ----------------------------------------------------------
# the input file to be processed by the tool
# ----------------------------------------------------------

input : "fullbarrests/FullBarRestWithoutBarLine.xml" ;


# ----------------------------------------------------------
# the options to the tool
# ----------------------------------------------------------

-auto-output-file-name

-trace-passes

-trace-measures
-trace-multiple-full-bar-rests

# -display-msr-1-summary

# -display-msr-1-short
-display-msr-2-short

# -display-lpsr-short

# -quit-after-pass-2b
-quit-after-pass-3

-cpu
